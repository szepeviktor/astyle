<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Clang ExampleSo" />
		<Option pch_mode="2" />
		<Option compiler="clang" />
		<Build>
			<Target title="Debug">
				<Option output="bin/ExampleSod" prefix_auto="1" extension_auto="1" />
				<Option object_output="objSo/Debug/" />
				<Option type="1" />
				<Option compiler="clang" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-std=c++11" />
					<Add option="-g" />
					<Add option="-W" />
					<Add option="-fno-rtti" />
					<Add option="-fno-exceptions" />
				</Compiler>
				<Linker>
					<Add library=":libastyled.so.$(SOLIBVER)" />
					<Add directory="bin" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/ExampleSo" prefix_auto="1" extension_auto="1" />
				<Option object_output="objSo/Release/" />
				<Option type="1" />
				<Option compiler="clang" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-Wall" />
					<Add option="-std=c++11" />
					<Add option="-W" />
					<Add option="-fno-rtti" />
					<Add option="-fno-exceptions" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library=":libastyle.so.$(SOLIBVER)" />
					<Add directory="bin" />
				</Linker>
			</Target>
			<Environment>
				<Variable name="SOLIBVER" value="3.2.0" />
			</Environment>
		</Build>
		<Unit filename="../../src-c/Example.cpp" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>

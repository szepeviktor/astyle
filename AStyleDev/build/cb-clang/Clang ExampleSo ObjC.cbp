<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Clang ExampleSo ObjC" />
		<Option pch_mode="2" />
		<Option default_target="Release" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/ExampleOCSod" prefix_auto="1" extension_auto="1" />
				<Option object_output="objSo/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-fconstant-string-class=NSConstantString" />
					<Add option="-D_NATIVE_OBJC_EXCEPTIONS" />
					<Add directory="/usr/include/GNUstep" />
				</Compiler>
				<Linker>
					<Add library="gnustep-base" />
					<Add library="objc" />
					<Add library=":libastyled.so.$(SOLIBVER)" />
					<Add directory="/usr/lib/GNUstep" />
					<Add directory="bin" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/ExampleOCSo" prefix_auto="1" extension_auto="1" />
				<Option object_output="objSo/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-fconstant-string-class=NSConstantString" />
					<Add option="-D_NATIVE_OBJC_EXCEPTIONS" />
					<Add directory="/usr/include/GNUstep" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-Wl,-rpath,./bin" />
					<Add library="gnustep-base" />
					<Add library="objc" />
					<Add library=":libastyle.so.$(SOLIBVER)" />
					<Add directory="/usr/lib/GNUstep" />
					<Add directory="bin" />
				</Linker>
			</Target>
			<Environment>
				<Variable name="SOLIBVER" value="3.1.0" />
			</Environment>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
		</Compiler>
		<Unit filename="../../src-o/Example.m">
			<Option compile="1" />
			<Option link="1" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>

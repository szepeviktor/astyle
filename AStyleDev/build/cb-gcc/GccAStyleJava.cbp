<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Gcc AStyle Java" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="AStyleJ">
				<Option output="bin/libastylej" prefix_auto="1" extension_auto="1" />
				<Option object_output="objJava" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="../test/*.cpp  ../test/*.h" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectLinkerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-Wall" />
					<Add option="-Wextra" />
					<Add option="-fno-rtti" />
					<Add option="-fno-exceptions" />
					<Add option="-fpic" />
					<Add option="-DASTYLE_JNI" />
					<Add option="-DNDEBUG" />
					<Add directory="/usr/lib/jvm/default-java/include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
				<ExtraCommands>
					<Add after="cp  bin/libastylej.so  ../../src-j" />
					<Add after="cp  bin/libastylej.so  ../../src-jx" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="AStyleJ;" />
		</VirtualTargets>
		<Unit filename="../../../AStyle/src/ASBeautifier.cpp" />
		<Unit filename="../../../AStyle/src/ASEnhancer.cpp" />
		<Unit filename="../../../AStyle/src/ASFormatter.cpp" />
		<Unit filename="../../../AStyle/src/ASResource.cpp" />
		<Unit filename="../../../AStyle/src/astyle.h" />
		<Unit filename="../../../AStyle/src/astyle_main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>

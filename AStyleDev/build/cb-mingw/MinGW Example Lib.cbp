<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="MinGW Example Lib" />
		<Option pch_mode="0" />
		<Option default_target="ExampleL" />
		<Option compiler="gcc" />
		<Build>
			<Target title="ExampleL">
				<Option output="bin/ExampleL.exe" prefix_auto="0" extension_auto="0" />
				<Option object_output="objLib" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectLinkerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-W" />
					<Add option="-fno-rtti" />
					<Add option="-fno-exceptions" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-Wl,--enable-auto-import" />
					<Add library="AStyleLib" />
					<Add directory="bin" />
				</Linker>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="ExampleL;" />
		</VirtualTargets>
		<Unit filename="../../src-c/Example.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>

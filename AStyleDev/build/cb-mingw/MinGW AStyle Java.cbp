<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="MinGW AStyle Java" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="AStyleJ">
				<Option output="bin/AStyle-3.0j.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="objJava" />
				<Option deps_output="dep" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectLinkerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-Wall" />
					<Add option="-W" />
					<Add option="-fno-rtti" />
					<Add option="-fno-exceptions" />
					<Add option="-DNDEBUG" />
					<Add option="-DASTYLE_JNI" />
					<Add directory="$(JAVA_HOME)/include" />
					<Add directory="$(JAVA_HOME)/include/win32" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-Wl,--add-stdcall-alias" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd /c  copy /y  bin\AStyle-3.0j.dll  ..\..\src-j\  &gt; NUL" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="AStyleJ;" />
		</VirtualTargets>
		<Unit filename="../../../AStyle/src/ASBeautifier.cpp" />
		<Unit filename="../../../AStyle/src/ASEnhancer.cpp" />
		<Unit filename="../../../AStyle/src/ASFormatter.cpp" />
		<Unit filename="../../../AStyle/src/ASResource.cpp" />
		<Unit filename="../../../AStyle/src/astyle.h" />
		<Unit filename="../../../AStyle/src/astyle_main.cpp" />
		<Unit filename="../../../AStyle/src/astyle_main.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>

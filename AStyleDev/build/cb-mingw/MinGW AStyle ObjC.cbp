<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="MinGW AStyle ObjC" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="AStyleO">
				<Option output="bin/AStyle" prefix_auto="1" extension_auto="1" />
				<Option object_output="objDll" />
				<Option deps_output="dep" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createStaticLib="1" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectLinkerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-fno-rtti" />
					<Add option="-fno-exceptions" />
					<Add option="-DNDEBUG" />
					<Add option="-DASTYLE_LIB" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-Wl,--add-stdcall-alias" />
					<Add directory="bin" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd /c  copy /y  bin\libAStyle.a ..\..\src-o\ &gt; NUL" />
					<Add after="cmd /c  copy /y  bin\AStyle.dll  ..\..\src-o\ &gt; NUL" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="Lint">
				<Option output="bin/AStylel" prefix_auto="1" extension_auto="1" />
				<Option object_output="objDlll/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Wshadow" />
					<Add option="-Winit-self" />
					<Add option="-Wredundant-decls" />
					<Add option="-Wcast-align" />
					<Add option="-Wundef" />
					<Add option="-Wfloat-equal" />
					<Add option="-Winline" />
					<Add option="-Wmissing-declarations" />
					<Add option="-Wmissing-include-dirs" />
					<Add option="-Wswitch-enum" />
					<Add option="-Wswitch-default" />
					<Add option="-Wmain" />
					<Add option="-pedantic" />
					<Add option="-std=c++0x" />
					<Add option="-std=c++98" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-fno-exceptions" />
					<Add option="-fno-rtti" />
					<Add option="-Wwrite-strings" />
					<Add option="-Woverloaded-virtual" />
					<Add option="-DASTYLE_LIB" />
				</Compiler>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="AStyleO;" />
		</VirtualTargets>
		<Unit filename="../../../AStyle/src/ASBeautifier.cpp" />
		<Unit filename="../../../AStyle/src/ASEnhancer.cpp" />
		<Unit filename="../../../AStyle/src/ASFormatter.cpp" />
		<Unit filename="../../../AStyle/src/ASResource.cpp" />
		<Unit filename="../../../AStyle/src/astyle.h" />
		<Unit filename="../../../AStyle/src/astyle_main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>

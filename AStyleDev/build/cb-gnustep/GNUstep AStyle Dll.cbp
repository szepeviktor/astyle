<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="GNUstep AStyle Dll" />
		<Option pch_mode="0" />
		<Option compiler="gnu_gcc_gnustep_compiler" />
		<Build>
			<Target title="AStyleD">
				<Option output="bin/AStyle30.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="objDll" />
				<Option deps_output="dep" />
				<Option type="3" />
				<Option compiler="gnu_gcc_gnustep_compiler" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectLinkerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-std=c++0x" />
					<Add option="-fno-rtti" />
					<Add option="-fno-exceptions" />
					<Add option="-DNDEBUG" />
					<Add option="-DASTYLE_LIB" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-Wl,--add-stdcall-alias" />
					<Add directory="bin" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd /c  copy /y  bin\AStyle30.dll  ..\..\src-o\  &gt; NUL" />
					<Add after="cmd /c  copy /y  bin\AStyle30.dll  ..\..\src-p\  &gt; NUL" />
					<Add after="cmd /c  copy /y  bin\AStyle30.dll  ..\..\src-s\  &gt; NUL" />
					<Add after="cmd /c  copy /y  bin\AStyle30.dll  ..\..\src-s2\ &gt; NUL" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="AStyleD;" />
		</VirtualTargets>
		<Unit filename="../../../AStyle/src/ASBeautifier.cpp" />
		<Unit filename="../../../AStyle/src/ASEnhancer.cpp" />
		<Unit filename="../../../AStyle/src/ASFormatter.cpp" />
		<Unit filename="../../../AStyle/src/ASResource.cpp" />
		<Unit filename="../../../AStyle/src/astyle.h" />
		<Unit filename="../../../AStyle/src/astyle_main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>

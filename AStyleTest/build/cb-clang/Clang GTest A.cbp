<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Clang GTest A" />
		<Option pch_mode="2" />
		<Option compiler="clang" />
		<Build>
			<Target title="Debug">
				<Option output="bin/gtestd" prefix_auto="1" extension_auto="1" />
				<Option working_dir="" />
				<Option object_output="obj/Debug/" />
				<Option type="2" />
				<Option compiler="clang" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-std=c++11" />
					<Add option="-g" />
					<Add option="-Wextra" />
					<Add option="-DGTEST_HAS_PTHREAD=0" />
					<Add directory="../../../googletest" />
					<Add directory="../../../googletest/include" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/gtest" prefix_auto="1" extension_auto="1" />
				<Option working_dir="" />
				<Option object_output="obj/Release/" />
				<Option type="2" />
				<Option compiler="clang" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-std=c++11" />
					<Add option="-Wextra" />
					<Add option="-DGTEST_HAS_PTHREAD=0" />
					<Add directory="../../../googletest" />
					<Add directory="../../../googletest/include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="Coverage" targets="Debug;" />
		</VirtualTargets>
		<Unit filename="../../../googletest/src/gtest-all.cc" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>

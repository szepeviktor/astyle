<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="MinGW AStyle" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="AStyle">
				<Option output="bin/AStyle" prefix_auto="0" extension_auto="1" />
				<Option object_output="obj" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="-A2tOP  ..\..\test-data\*.cpp  ..\..\test-data\*.h" />
				<Option projectCompilerOptionsRelation="1" />
				<Option projectLinkerOptionsRelation="1" />
				<Option projectIncludeDirsRelation="1" />
				<Option projectResourceIncludeDirsRelation="1" />
				<Option projectLibDirsRelation="1" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-std=c++11" />
					<Add option="-fno-rtti" />
					<Add option="-fno-exceptions" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd /c  copy /y  bin\AStyle.exe ..\..\src-p\ &gt; NUL" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="AStyle;" />
		</VirtualTargets>
		<Unit filename="../../../AStyle/src/ASBeautifier.cpp" />
		<Unit filename="../../../AStyle/src/ASEnhancer.cpp" />
		<Unit filename="../../../AStyle/src/ASFormatter.cpp" />
		<Unit filename="../../../AStyle/src/ASLocalizer.cpp" />
		<Unit filename="../../../AStyle/src/ASResource.cpp" />
		<Unit filename="../../../AStyle/src/astyle.h" />
		<Unit filename="../../../AStyle/src/astyle_main.cpp" />
		<Unit filename="../../../AStyle/src/astyle_main.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>

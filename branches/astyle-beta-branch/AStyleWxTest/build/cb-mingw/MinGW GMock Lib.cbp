<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="MinGW GMock Lib" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="lib/gmockd" prefix_auto="1" extension_auto="1" />
				<Option working_dir="" />
				<Option object_output="obj/Debug/" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-Wno-unused-function" />
					<Add option="-Wno-unused-variable" />
					<Add directory="../../../googlemock" />
					<Add directory="../../../googlemock/include" />
					<Add directory="../../../googletest" />
					<Add directory="../../../googletest/include" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="lib/gmock" prefix_auto="1" extension_auto="1" />
				<Option working_dir="" />
				<Option object_output="obj/Release/" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-Wno-unused-function" />
					<Add option="-Wno-unused-variable" />
					<Add option="-DNDEBUG" />
					<Add directory="../../../googlemock" />
					<Add directory="../../../googlemock/include" />
					<Add directory="../../../googletest" />
					<Add directory="../../../googletest/include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="DLL Debug" targets="Debug;" />
			<Add alias="DLL Release" targets="Release;" />
		</VirtualTargets>
		<Unit filename="../../../googletest/src/gtest-all.cc" />
		<Unit filename="../../../googlemock/src/gmock-all.cc" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
